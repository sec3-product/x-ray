elif test "$enable_http" = "shared"; then
    { { echo "$as_me:$LINENO: error: mod_http can not be built as a shared DSO" >&5
echo "$as_me: error: mod_http can not be built as a shared DSO" >&2;}
   { (exit 1); exit 1; }; }
