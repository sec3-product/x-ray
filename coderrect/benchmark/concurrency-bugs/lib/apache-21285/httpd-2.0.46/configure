#line $LINENO "configure"
#include "confdefs.h"

#include <apr.h>
#if APR_HAS_DSO
YES_IS_DEFINED
#endif

_ACEOF
if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  $EGREP "YES_IS_DEFINED" >/dev/null 2>&1; then
  ac_cv_define_APR_HAS_DSO=yes
else
  ac_cv_define_APR_HAS_DSO=no
fi
rm -f conftest*

